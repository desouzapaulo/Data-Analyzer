classdef LocationClass < handle
    properties
        t = []
        speed = [];
    end
    methods
        function obj = LocationClass(t, speed)
            obj.t = t;
            obj.speed = speed;
        end
        function speed(obj, n)
            figure(n)
            plot(obj.t, obj.a)
            xlabel('time [s]')
            ylabel('Acceleration [g]')
            grid on
        end
    end
end